{
  "name": "my-special-way-server",
  "version": "0.0.164",
  "description": "server-for-my-special-way-android/portal",
  "author": "my-special-way",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/myspecialway/my-special-way-server"
  },
  "scripts": {
    "build": "bash ./scripts/build",
    "bump": "yarn version --new-version patch",
    "docker": "bash ./scripts/docker",
    "docker:push": "bash ./scripts/docker-push",
    "docker:deploy": "bash ./scripts/k8s/deploy",
    "format": "prettier --write \"**/*.ts\"",
    "git:push": "./scripts/git-push",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:watch": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && tsc",
    "mongo-shell": "mongo mongodb://localhost:27017/msw-dev",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "test:ut": "jest --coverage --detectOpenHandles",
    "test:e2e": "docker-compose up -d && cross-env NODE_ENV=e2e-tests jest --config ./jest-e2e.json && docker rm -f $(docker ps -q)",
    "test:ci": "bash ./scripts/test",
    "test:watch": "jest --watch",
    "tslint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.ts'",
    "webpack": "webpack --config webpack.config.js",
    "stryker": "npx stryker run",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/app/**/*{.ts, .scss}\"",
    "seed": "ts-node ./db-seed/seed.ts -c mongodb://localhost:27017",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "@nestjs/common": "^5.0.0-beta.6",
    "@nestjs/core": "^5.0.0-beta.6",
    "@nestjs/graphql": "^3.0.0",
    "@nestjs/microservices": "^5.0.0-beta.6",
    "@nestjs/passport": "^1.0.10",
    "@nestjs/testing": "^5.0.0-beta.6",
    "@nestjs/websockets": "^5.0.0-beta.6",
    "apollo-server-express": "^1.3.6",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^6.0.0",
    "envalid": "^4.1.4",
    "fastify-formbody": "^2.0.0",
    "firebase-admin": "^6.1.0",
    "global": "^4.3.2",
    "graphiql": "^0.11.11",
    "graphql": "^0.13.2",
    "graphql-playground-middleware-express": "^1.7.0",
    "graphql-tools": "^3.0.2",
    "joi": "14.3.1",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "mongodb": "^3.0.10",
    "nest-winston": "^1.0.2",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0",
    "snyk": "^1.161.1",
    "typescript": "^2.8.3",
    "winston": "^3.1.0",
    "winston-papertrail": "^1.0.5"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.6.0",
    "@types/argparse": "^1.0.34",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.0.39",
    "@types/graphql": "^0.13.1",
    "@types/jest": "^21.1.8",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/lodash": "^4.14.110",
    "@types/mongodb": "^3.0.19",
    "@types/node": "^9.3.0",
    "@types/nodemailer": "^4.6.5",
    "@types/passport": "^0.4.5",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.4",
    "argparse": "^1.0.10",
    "codecov": "^3.7.1",
    "cpy-cli": "^2.0.0",
    "cross-env": "^5.2.0",
    "husky": "^1.1.2",
    "jest": "^23.0.1",
    "mongodb-extjson": "^3.0.3",
    "nodemon": "^1.14.1",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "stryker": "^0.24.1",
    "stryker-api": "^0.17.2",
    "stryker-html-reporter": "^0.14.2",
    "stryker-jest-runner": "^0.7.0",
    "stryker-typescript": "^0.11.2",
    "supertest": "^3.0.0",
    "ts-jest": "22.4.4",
    "ts-loader": "^4.1.0",
    "ts-node": "^6.1.1",
    "tsconfig-paths": "^3.1.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "5.10.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-node-externals": "^1.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format:fix",
      "pre-push": "yarn tslint"
    }
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "ignoreCoverageForAllDecorators": true,
        "ignoreCoverageForDecorators": true
      }
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "**/*.ts",
      "!main.ts",
      "!main.hmr.ts",
      "!**/*.module.ts",
      "!**/*.temp.ts",
      "!**/*.model.ts",
      "!config/**/*.ts"
    ]
  },
  "snyk": true
}
